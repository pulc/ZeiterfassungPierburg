@{
    Layout = "";
    ViewBag.Title = "Error";
}

<section class="content">

    <div class="error-page">
        <h2 class="headline text-red">☹</h2>

        <div class="error-content">
            <h3><i class="fa fa-warning text-red"></i> Du hast keinen Zugriff auf diese Seite.</h3>

            <p>
                Kontaktiere den Admin, um die Rechte zu bekommen.

            </p>

            <p>
                <button class="btn-toolbar" onclick="goBack()">Zurück auf die vorige Seite</button>
            </p>
        </div>
    </div>
    <!-- /.error-page -->

</section>

<script>
    function goBack() {
        window.history.back();
    }
</script>